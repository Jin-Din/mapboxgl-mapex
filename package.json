{
  "name": "@jindin/mapboxgl-mapex",
  "description": "mapboxgl地图对象功能扩展(可适用于天地图陕西的mapbox-gl api)",
  "private": false,
  "version": "2.0.1",
  "type": "module",
  "files": [
    "dist",
    "package.json"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "release": "vue-tsc && npm publish --access public"
  },
  "devDependencies": {
    "rollup-plugin-javascript-obfuscator": "^1.0.4",
    "terser": "^5.17.7",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vue-tsc": "^1.6.5"
  },
  "dependencies": {
    "@types/mapbox-gl": "^2.7.11",
    "@types/node": "^20.3.0",
    "mapbox-gl": "^2.15.0",
    "vite-plugin-dts": "^2.3.0"
  }
}
